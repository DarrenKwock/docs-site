{
  "index_name": "staging_docs_couchbase",
  "start_urls": [
    {
      "url": "https://cb-docs-sandbox.netlify.app/(?P<component>.*?)/(?P<version>.*?)/",
      "variables": {
        "component": [
          "operator"
        ],
        "version": [
          "2.0",
          "1.2",
          "1.1",
          "1.0"
        ]
      }
    }
  ],
  "sitemap_urls": [
    "https://cb-docs-sandbox.netlify.app/sitemap.xml"
  ],
  "scrape_start_urls": false,
  "stop_urls": [],
  "selectors": {
    "default": {
      "lvl0": {
        "global": true,
        "selector": ".component-frame .frame-body"
      },
      "lvl1": ".doc > h1.page",
      "lvl2": ".doc .sect1 > h2:first-child",
      "lvl3": ".doc .sect2 > h3:first-child",
      "lvl4": ".doc .sect3 > h4:first-child",
      "text": ".doc p, .doc dt"
    }
  },
  "selectors_exclude": [
    ".doc .admonitionblock",
    ".doc table.tableblock"
  ],
  "min_indexed_level": 1,
  "custom_settings": {
    "attributesForFaceting": [
      "component",
      "version"
    ],
    "attributesToRetrieve": [
      "anchor",
      "content",
      "hierarchy",
      "url",
      "component",
      "version"
    ],
    "customRanking": [
      "desc(weight.page_rank)",
      "desc(version)",
      "desc(weight.level)",
      "asc(weight.position)"
    ]
  }
}
